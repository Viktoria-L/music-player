{
  "homepage": "https://viktoria-l.github.io/music-player",
  "name": "music-player",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "server": "nodemon --watch backend backend/server.js",
    "dev:all": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd Frontend && npm run build",
    "deploy": "npm run build && npx gh-pages -d frontend/dist -b gh-pages -f"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "gh-pages": "^6.1.1",
    "nodemon": "^3.1.0"
  }
}
